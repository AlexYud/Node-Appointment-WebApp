<script>
  var element = document.getElementById("calendar")
  var calendar = new FullCalendar.Calendar(element, {
    initialView: 'dayGridMonth',
    locale: 'pt-br',
    events: 'getcalendar',
    eventClick: function (info) {
      window.location.href = `/event/${info.event.id}`
    }
  })
  calendar.render()
</script>
